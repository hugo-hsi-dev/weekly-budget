		"tailwindcss": "^4.1.8",
		"tw-animate-css": "^1.3.4",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.34.0",
		"vite": "^6.3.5",
		"vite-plugin-devtools-json": "^0.2.0"
	},
	"dependencies": {
		"better-sqlite3": "^11.10.0",
		"drizzle-orm": "^0.44.2"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild",
			"better-sqlite3"
		]
